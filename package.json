{
  "name": "scaflow",
  "version": "0.0.5",
  "description": "Scaffold your projects with your own templates",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "local-render": "node ./index.js -t ./output/templates/webapp -y ./tests/data -o output/rendered",
    "local-wrap": "node ./index.js --wrap -t ./templates/webapp -y ./tests/data -o output/templates/webapp",
    "scf": "scaflow -t ./templates/webapp -y ./tests/data -o output/webapp",
    "scfw": "scaflow --wrap -t ./templates/webapp -y ./tests/data -o output/templates",
    "build": "npm i",
    "test": "node --test --inspect ./tests/",
    "test:coverage": "c8 node --test"
  },
  "author": "larrylane",
  "license": "MIT",
  "dependencies": {
    "commander": "^13.1.0",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "c8": "^10.1.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "scaflow": "./index.js"
  } 
}